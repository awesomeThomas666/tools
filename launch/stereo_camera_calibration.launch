<!-- Launch in the camera namespace containing "image_raw" and "camera_info" -->
<launch>

  <node pkg="camera_calibration" type="cameracalibrator.py" name="camera_calibration" args="--size 6x9 --square 0.025 right:=/my_stereo/right/image_raw left:=/my_stereo/left/image_raw left_camera:=/my_stereo/left right_camera:=/my_stereo/right --no-service-check" output="screen">
  </node>  

</launch>
